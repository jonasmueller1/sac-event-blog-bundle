{
    "name": "markocupic/sac-event-blog-bundle",
    "description": "Manage event blogs in the Contao backend and display them in the frontend of your website.",
    "keywords": [
        "SAC",
        "Swiss Alpine Club",
        "Contao",
        "event",
        "blog"
    ],
    "type": "contao-bundle",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Marko Cupic",
            "email": "m.cupic@gmx.ch",
            "homepage": "https://github.com/markocupic/event-blog-bundle",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/markocupic/sac-event-blog-bundle/issues",
        "source": "https://github.com/markocupic/sac-event-blog-bundle"
    },
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^5.3",
        "chillerlan/php-qrcode": "^5.0",
        "codefog/contao-haste": "^5.0",
        "markocupic/contao-component-vue-js": "^3.3",
        "markocupic/contao-twig-assets": "^1.0",
        "markocupic/phpoffice-bundle": "^1.3",
        "markocupic/sac-event-tool-bundle": "dev-contao5",
        "markocupic/zip-bundle": "^1.1",
        "terminal42/notification_center": "^2.0",
        "terminal42/contao-fineuploader": "^3.4"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.4",
        "contao/test-case": "^5.3",
        "phpunit/phpunit": "^9.3",
        "contao/easy-coding-standard": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Markocupic\\SacEventBlogBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Markocupic\\SacEventBlogBundle\\ContaoManager\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "contao-components/installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "contao-community-alliance/composer-plugin": true,
            "contao/manager-plugin": true
        }
    }
}
